{
  "name": "stellar.org",
  "private": true,
  "version": "0.0.0",
  "scripts": {
    "start": "lingui compile; gatsby develop",
    "lint": "eslint --ext .js,.jsx --ignore-pattern public .",
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "lingui compile; gatsby build",
    "check-not-master": "[ \"$(git rev-parse --abbrev-ref HEAD)\" = \"master\" ] && echo \"\\033[31mDon't commit to master\" && exit 1 || exit 0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "concurrently 'yarn check-not-master' 'pretty-quick --staged' 'lingui extract; git add */locale/*/*.json'"
    }
  },
  "dependencies": {
    "@lingui/cli": "^2.7.4",
    "@lingui/core": "^2.7.4",
    "@lingui/macro": "^2.7.4",
    "@lingui/react": "^2.7.4",
    "@mdx-js/mdx": "^1.0.15",
    "@mdx-js/react": "^1.0.6",
    "@mdx-js/tag": "^0.20.3",
    "@reach/router": "^1.2.1",
    "@stellar/eslint-config": "^1.0.0",
    "@stellar/prettier-config": "^1.0.1",
    "core-js": "2",
    "dotenv": "^7.0.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-config-prettier": "^4.2.0",
    "eslint-config-react-app": "^4.0.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react-hooks": "^1.6.0",
    "fetch-jsonp": "^1.1.3",
    "gatsby": "^2.3.30",
    "gatsby-image": "^2.0.40",
    "gatsby-mdx": "^0.6.2",
    "gatsby-plugin-eslint": "^2.0.5",
    "gatsby-plugin-react-helmet": "^3.0.0",
    "gatsby-plugin-react-svg": "^2.1.1",
    "gatsby-plugin-sharp": "^2.0.35",
    "gatsby-plugin-sitemap": "^2.1.0",
    "gatsby-plugin-styled-components": "^3.0.7",
    "gatsby-source-contentful": "^2.0.53",
    "gatsby-source-filesystem": "^2.0.32",
    "gatsby-transformer-remark": "^2.3.12",
    "gatsby-transformer-sharp": "^2.1.18",
    "hast-to-hyperscript": "^7.0.0",
    "lodash": "^4.17.11",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-helmet": "^5.2.0",
    "react-transition-group": "^4.0.1",
    "sharp": "^0.22.0",
    "styled-components": "^4.2.0",
    "styled-normalize": "^8.0.6",
    "tinycolor2": "^1.4.1",
    "url": "^0.11.0",
    "whatwg-fetch": "^2.0.4"
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/plugin-proposal-class-properties": "^7.4.0",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-react": "^7.0.0",
    "babel-core": "^7.0.0-bridge.0",
    "babel-eslint": "^10.0.1",
    "babel-plugin-macros": "^2.5.1",
    "babel-plugin-styled-components": "^1.10.0",
    "concurrently": "^4.1.0",
    "contentful-import": "^7.4.0",
    "eslint": "^5.6.1",
    "eslint-loader": "^2.1.2",
    "eslint-plugin-react": "^7.11.1",
    "husky": "^2.1.0",
    "prettier": "^1.17.0",
    "pretty-quick": "^1.10.0"
  },
  "homepage": "https://stellar.org",
  "license": "MIT",
  "prettier": "@stellar/prettier-config",
  "lingui": {
    "fallbackLocale": "",
    "sourceLocale": "en",
    "localeDir": "<rootDir>/src/locale",
    "srcPathDirs": [
      "<rootDir>/src"
    ],
    "srcPathIgnorePatterns": [
      "/node_modules/"
    ],
    "format": "lingui"
  }
}
